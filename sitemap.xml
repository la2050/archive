    <loc>https://archive.la2050.org{{ organization.url }}</loc>
  </url>
{% endfor %}

{% comment %}
######### Projects #########
{% endcomment %}

{% assign data_collection = site.collections | where: "label", "projects" | first %}
{% assign data_list = data_collection.docs %}
{% for project in data_list %}
